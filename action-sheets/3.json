{
  "id": "test-sheet",
  "name": "test-sheet",
  "userInputs": [
    {
      "token": "userId"
    },
    {
      "token": "password"
    },
    {
      "token": "qtyCondition"
    }
  ],
  "actions": {
    "1": {
      "type": "launch",
      "site": "https://crmdms.inservices.tatamotors.com/workshop/start.swe?"
    },
    "2": {
      "type": "wait",
      "duration": 10000
    },
    "3": {
      "type": "login",
      "fields": [
        {
          "type": "text",
          "selector": "#s_swepi_1",
          "value": "",
          "useUserInput": true,
          "inputToken": "userId"
        },
        {
          "type": "text",
          "selector": "#s_swepi_2",
          "value": "",
          "useUserInput": true,
          "inputToken": "password"
        }
      ],
      "submit": {
        "selector": "#s_swepi_22"
      }
    },
    "4": {
      "type": "wait",
      "duration": 5000
    },
    "5": {
      "type": "click",
      "selector": "span.siebui-icon-tb-sitemap.ToolbarButtonOn",
      "description": "Click Site Map button"
    },
    "6": {
      "type": "wait",
      "duration": 2000
    },
    "7": {
      "type": "type",
      "selector": "#sitemapFilterInput",
      "value": "MIS - Spares",
      "description": "Type the link name into the search box"
    },
    "8": {
      "type": "wait",
      "duration": 2000,
      "description": "Wait for search results to appear"
    },
    "9": {
      "type": "click",
      "selector": "//a[contains(text(), '{{searchText}}')]",
      "isXPath": true,
      "description": "Click on the result in the dropdown list",
      "searchText": "MIS - Spares"
    },
    "10": {
      "type": "wait",
      "duration": 10000
    },
    "11": {
      "type": "click",
      "selector": "#s_1_1_16_0_Ctrl",
      "description": "Click search icon button"
    },
    "12": {
      "type": "wait",
      "duration": 2000,
      "description": "Wait briefly after clicking Query button"
    },
    "13": {
      "type": "click",
      "selector": "#\\31 _s_1_l_Quantity",
      "description": "Click the Quantity cell to open input"
    },
    "14": {
      "type": "type",
      "selector": "#\\31 _Quantity",
      "value": ">0",
      "description": "Enter quantity condition in the input field"
    },
    "15": {
      "type": "click",
      "selector": "#\\31 _s_1_l_Availability",
      "description": "Click the search icon button to apply filter"
    },
    "16": {
      "type": "type",
      "selector": "#\\31 _Availability",
      "value": "\"On Hand\"",
      "description": "Enter availability condition in the input field"
    },
    "17": {
      "type": "keyboard",
      "key": "Enter",
      "description": "Press Enter after typing in the quantity field"
    },
    "18": {
      "type": "wait",
      "duration": 5000,
      "description": "Wait briefly after hitting Enter button"
    },
    "19": {
      "type": "click",
      "selector": "#s_1_1_21_0_Ctrl",
      "description": "Click the download Excel button"
    },
    "20": {
      "type": "wait",
      "duration": 5000,
      "description": "Wait for Export popup to appear"
    },
    "21": {
      "type": "click",
      "selector": "#s_2_1_54_0_Ctrl",
      "description": "Click on 'Next' button in the Export popup"
    },
    "22": {
      "type": "wait",
      "duration": 5000,
      "description": "Wait for export to process"
    },
    "23": {
      "type": "click",
      "selector": "#s_2_1_55_0_Ctrl",
      "description": "Click on 'close' button in the Export popup"
    },
    "24": {
      "type": "wait",
      "duration": 5000
    },
    "25": {
      "type": "logout",
      "steps": [
        {
          "description": "Click on Settings",
          "selector": "li#tb_0"
        },
        {
          "description": "Click on Logout",
          "selector": "button.siebui-ctrl-btn.appletButton.siebui-btn-logout"
        }
      ]
    },
    "26": {
      "type": "wait",
      "duration": 5000
    },
    "27": {
      "type": "close"
    }
  }
}
