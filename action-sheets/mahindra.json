{
  "id": "mahindra-001",
  "name": "Mahindra Report",
  "userInputs": [
    {
      "token": "username",
      "type": "text"
    },
    {
      "token": "password",
      "type": "password"
    }
  ],
  "actions": {
    "1": {
      "type": "launch",
      "site": "https://www.example.com/login"
    },
    "2": {
      "type": "login",
      "fields": [
        {
          "selector": "#username",
          "inputToken": "username",
          "useUserInput": true
        },
        {
          "selector": "#password",
          "inputToken": "password",
          "useUserInput": true
        }
      ],
      "submit": {
        "selector": "#submit"
      }
    }
  }
}
